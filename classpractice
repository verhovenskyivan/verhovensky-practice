using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
 
namespace ConsoleApplication2
{
class Animal // общие параметры для животных
{
protected string name;
}
 
class dog : Animal
{
public void debug()
{
name = "рэкс";
Console.WriteLine(name);
}
}
 
class cat : Animal
{
public void debug()
{
name = "барсик";
Console.WriteLine(name);
}
 
}
 
class Program
{
static void Main (string[] args) // вызывается по старту программы
{
//создание функции отмечать модификаторами доступа экземпляров классов cat и dog
cat Cat = new cat();
dog Dog = new dog();
 
//вызов функии debug
Cat.debug();
Dog.debug();
 
//команда задержки консоли
Console.ReadKey();
}
}
}
